<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>


<BorderPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StudentManagerTransactionController">
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button mnemonicParsing="false" onAction="#onEnter" text="Enter" />
            <Button mnemonicParsing="false" onAction="#onReturn" text="Cancel" />
        </buttons>
      </ButtonBar>
   </bottom>
   <top>
      <Label text="STUDENT ACCOUNT TRANSACTION" BorderPane.alignment="CENTER_LEFT">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
   </top>
   <center>
      <AnchorPane prefHeight="222.0" prefWidth="544.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="ID" />
            <Label fx:id="idLabel" layoutX="116.0" layoutY="12.0" text="ID">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="42.0" text="Name" />
            <Label fx:id="nameLabel" layoutX="116.0" layoutY="40.0" text="Name">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Separator layoutX="8.0" layoutY="163.0" prefWidth="200.0" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" />
            <Label layoutX="14.0" layoutY="72.0" text="Balance" />
            <Label fx:id="balanceLabel" layoutX="116.0" layoutY="70.0" text="Balance">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="102.0" text="Amount Paid" />
            <Label fx:id="amountPaidLabel" layoutX="116.0" layoutY="100.0" text="Amount Paid">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="132.0" text="Tuition Fee" />
            <Label fx:id="tuitionFeeLabel" layoutX="116.0" layoutY="130.0" text="Tuition Fee">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="196.0" text="Enter Amount" />
            <Label layoutX="208.0" layoutY="176.0" text="TRANSACTION">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Spinner fx:id="amountField" editable="true" layoutX="12.0" layoutY="221.0" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" />
         </children>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </AnchorPane>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
